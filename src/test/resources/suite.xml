<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Test suite ESPN">

    <listeners>
        <listener class-name="org.espn.utils.Listener"/>
    </listeners>

    <parameter name="url" value="https://www.espnqa.com/?src=com&amp;_adblock=true&amp;espn=cloud"></parameter>
    <parameter name="browser" value="chrome"></parameter>


    <test name="Espn Test" enabled="true">
        <classes>
            <class name="org.espn.tests.EspnLoginTest"></class>
            <class name="org.espn.tests.EspnLogoutTest"></class>
            <class name="org.espn.tests.EspnDeactivateAccountTest"></class>
        </classes>
    </test>

</suite>
